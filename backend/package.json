    "db:seed": "node prisma/seed.js"
  },
  "engines": {
    "node": ">=18 <=22"
  },
  "dependencies": {
    "@prisma/client": "6.16.1",
    "axios": "1.12.1",
    "bcrypt": "6.0.0",
    "cors": "2.8.5",
    "dotenv": "17.2.2",
    "express": "5.1.0",
    "jsonwebtoken": "9.0.2",
    "node-cron": "4.2.1",
    "prisma": "6.16.1"
  },
  "devDependencies": {
    "@types/bcrypt": "6.0.0",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.0",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "20.14.12",
    "ts-node-dev": "2.0.0",
    "typescript": "5.6.3"
  }
}

